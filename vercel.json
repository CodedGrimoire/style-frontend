{
  "version": 2,
  "routes": [
    {
      "src": "/old-route",
      "dest": "/new-route"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    }
  ],
  "builds": [
    {
      "src": "pages/**/*",
      "use": "@vercel/next"
    }
  ],
  "env": {
    "MY_SECRET": "your-secret-value"
  }
}
